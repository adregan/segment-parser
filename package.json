{
  "name": "eave-takehome",
  "version": "1.0.0",
  "description": "A metro 2 format parser",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "husky": "^0.14.2",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.2",
    "ts-jest": "^20.0.6",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs"
      }
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "lint-staged": {
    "*.(js|ts)": [
      "prettier --write",
      "git add -p"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adregan/eave.git"
  },
  "author": "Duncan Regan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adregan/eave/issues"
  },
  "homepage": "https://github.com/adregan/eave#readme"
}
